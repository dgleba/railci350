        <td><%= cilisting1.il4_target_date %></td>
        <td><%= cilisting1.il5_target_date %></td>
        <td><%= cilisting1.wc_idea_date %></td>
        <td><%= cilisting1.actual_implementation_date %></td>
        <td><%= cilisting1.enter_in_wc %></td>
        <td><%= cilisting1.area %></td>
        <td><%= cilisting1.updatedtime %></td>
        <td><%= cilisting1.id_wc %></td>
        <td><%= cilisting1.environmental %></td>
        <td><%= cilisting1.ease_of_implementation %></td>
        <td><%= cilisting1.submit %></td>
        <td><%= cilisting1.next_steps %></td>
        <td><%= cilisting1.suggestion_status %></td>
        <td><%= cilisting1.suggestion_status_updated_at %></td>
        <td><%= cilisting1.grouping %></td>
        <td><%= cilisting1.metric_impact %></td>
        <td><%= cilisting1.gift2_4_suggestion %></td>
        <td><%= cilisting1.gift_4_suggestion %></td>
        <td><%= cilisting1.il_current %></td>
