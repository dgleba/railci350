        <td><%= cilisting1.link %></td>
        <td><%= cilisting1.linkmore %></td>
        <td><%= cilisting1.display %></td>
        <td><%= cilisting1.ciid %></td>
      </tr>
    <% end %>
  </tbody>
</table>

