<%= simple_form_for(@ci_review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_number %>
    <%= f.input :suggestion_status %>
    <%= f.input :owner %>
    <%= f.input :project_description %>
    <%= f.input :comments %>
    <%= f.input :originator %>
    <%= f.input :link %>
    <%= f.input :linkmore %>
    <%= f.input :team %>
    <%= f.input :wc_idea_date %>
    <%= f.input :ciid %>
    <%= f.input :updatedtime %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-success'%>
  </div>
<% end %>
